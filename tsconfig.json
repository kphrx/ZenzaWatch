{
  "ts-node": { "files": true },
  "compilerOptions": {
    "target": "es5",
    "module": "esnext",
    "lib": ["esnext"],
    "outDir": "./dist/",
    "downlevelIteration": true,
    "strict": true,
    "moduleResolution": "node",
    "baseUrl": "src",
    "paths": {
      "@zenza/*": ["./*"]
    },
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true
  }
}
