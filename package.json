{
  "name": "nico-zenza",
  "version": "0.0.0",
  "description": "niconico HTML5 Player",
  "main": "",
  "type": "module",
  "scripts": {
    "check-type": "tsc --noEmit -p .",
    "lint": "eslint --ext .ts,.js .",
    "lint:fix": "eslint --fix --ext .ts,.js .",
    "build:dev": "NODE_OPTIONS=\"--loader ts-node/esm --experimental-specifier-resolution=node\" webpack --env development",
    "build:prod": "NODE_OPTIONS=\"--loader ts-node/esm --experimental-specifier-resolution=node\" webpack --env production",
    "test": "NODE_ENV=test mocha test/**/*.ts",
    "test:watch": "NODE_ENV=test mocha --watch test/**/*.ts",
    "postinstall": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kPherox/ZenzaWatch.git"
  },
  "author": "kphrx",
  "license": "WTFPL",
  "bugs": {
    "url": "https://github.com/kPherox/ZenzaWatch/issues"
  },
  "homepage": "https://github.com/kPherox/ZenzaWatch#readme",
  "devDependencies": {
    "@types/mocha": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "eslint": "^8.5.0",
    "eslint-config-google": "^0.14.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.4",
    "mocha": "^9.1.3",
    "ts-loader": "^9.1.1",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.9.0",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "typescript": "^4.2.4",
    "webpack": "^5.36.2",
    "webpack-cli": "^4.6.0",
    "webpack-merge": "^5.7.3"
  }
}
